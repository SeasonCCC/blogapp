    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "mongodb": "^3.3.4",
    "prettier": "^1.18.2",
    "standard": "^14.3.1",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
